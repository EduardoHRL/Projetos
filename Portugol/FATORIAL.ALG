Algoritmo "Subsequente"
Var
   N, Cont, T : INTEIRO
inicio
      escreva (" Digite um número:")
      Leia (N)
      Cont <- 1
      T <- N
      enquanto (Cont <= 20) faca
               T <- T+1
               Cont <- Cont + 1
               Escreval ("os 20 numeros subsequentes é: ", T)
      fimenquanto
fimalgoritmo